  ],
  "keywords": [
    "vitest",
    "notion",
    "reporter",
    "test",
    "documentation"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=18.0.0"
  },
