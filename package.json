  "homepage": "https://github.com/swan-io/vitest-notion-reporter",
  "repository": {
    "type": "git",
    "url": "https://github.com/swan-io/vitest-notion-reporter.git"
  },
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
